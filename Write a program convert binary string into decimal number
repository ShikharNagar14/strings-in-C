#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main()
{
    char str[] = "1001";
    int i = strlen(str) - 1;
    int res = 0;
    int base = 1;
    while (1 >= 0)
    {
        int x = str[i] - 48;
        res = res + x * base;
        base = base * 2;
        i--;
    }
    printf("%d",res);
}
